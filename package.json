{
	"name": "z-interact",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:wrangler": "wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-01-01",
		"dev:local": "npm run build && wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2024-01-01 --live-reload",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-cloudflare": "^7.2.3",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^22",
		"@types/qrcode": "^1.5.5",
		"drizzle-kit": "^0.31.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.25.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"wrangler": "^4.33.2"
	},
	"dependencies": {
		"@fal-ai/client": "^1.2.0",
		"@libsql/client": "^0.14.0",
		"@neoconfetti/svelte": "^2.2.2",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.40.0",
		"drizzle-valibot": "^0.4.2",
		"flowbite": "^3.1.2",
		"flowbite-svelte": "^1.13.7",
		"flowbite-svelte-icons": "^3.0.0",
		"qrcode": "^1.5.4",
		"tailwind-merge": "^3.3.1",
		"valibot": "^1.1.0"
	}
}
